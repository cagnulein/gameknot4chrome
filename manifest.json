{
  "name": "gameknot4chrome",
  "version": "1.0",
  "description": "The first extension for Gameknot.com by Cagnulein (http://cagnulein.no-ip.info/wordpress/)",
  "icons": {"16": "icon.png","48": "logo.jpg","128": "logo.jpg"},
